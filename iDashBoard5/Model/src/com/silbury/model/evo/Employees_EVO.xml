  <SQLQuery><![CDATA[SELECT EmployeesEO22.COMMISSION_PCT,   
       EmployeesEO22.DEPARTMENT_ID,   
       EmployeesEO22.EMAIL,   
       EmployeesEO22.EMPLOYEE_ID,   
       EmployeesEO22.FIRST_NAME,   
       EmployeesEO22.HIRE_DATE,   
       EmployeesEO22.JOB_ID,   
       EmployeesEO22.LAST_NAME,   
       EmployeesEO22.MANAGER_ID,   
       EmployeesEO22.PHONE_NUMBER,   
       EmployeesEO22.SALARY,   
       DepartmentsEO22.DEPARTMENT_NAME,   
       DepartmentsEO22.DEPARTMENT_ID AS DEPARTMENT_ID1,   
       Jobs.JOB_TITLE,   
       Jobs.JOB_ID AS JOB_ID1, 
       Employees.MANAGER_ID AS MANAGER_ID1,   
       (Select Employees.FIRST_NAME ||' '|| Employees.LAST_NAME from EMPLOYEES Employees where Employees.EMPLOYEE_ID = EmployeesEO22.MANAGER_ID) FIRST_NAME1,   
       Employees.LAST_NAME AS LAST_NAME1,   
       Employees.EMPLOYEE_ID AS EMPLOYEE_ID1  
FROM  EMPLOYEES EmployeesEO22, DEPARTMENTS DepartmentsEO22, JOBS Jobs, EMPLOYEES Employees  
WHERE (EmployeesEO22.DEPARTMENT_ID = DepartmentsEO22.DEPARTMENT_ID) AND (Jobs.JOB_ID = EmployeesEO22.JOB_ID)   
and (Employees.EMPLOYEE_ID = EmployeesEO22.EMPLOYEE_ID)]]></SQLQuery>
