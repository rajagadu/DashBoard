  <SQLQuery><![CDATA[SELECT        
EDV.EMPLOYEE_ID,       
EDV.FIRST_NAME,       
EDV.LAST_NAME,       
EDV.JOB_TITLE,       
EDV.DEPARTMENT_ID,       
EDV.DEPARTMENT_NAME,       
EMP.PHONE_NUMBER,       
EMP.PHONE_NUMBER,       
EMP.EMAIL,       
EMP.FIRST_NAME LINKEDIN,       
EMP.FIRST_NAME TWITTER,       
EMP.FIRST_NAME FACEBOOK,       
EMP.FIRST_NAME GOOGLE,       
LOC.STREET_ADDRESS,       
EDV.CITY,       
EDV.STATE_PROVINCE,       
EDV.COUNTRY_NAME,       
LOC.POSTAL_CODE,       
'' RATING,       
'' POTENTIAL,       
'' COMPRATIO,       
EDV.SALARY,       
'' BOUNS,       
EMP.COMMISSION_PCT,       
EDV.MANAGER_ID,       
EMP.HIRE_DATE,       
(SELECT FIRST_NAME FROM EMPLOYEES WHERE EMPLOYEE_ID = EDV.MANAGER_ID) MGR_FIRST_NM,       
(SELECT LAST_NAME FROM EMPLOYEES WHERE EMPLOYEE_ID = EDV.MANAGER_ID) MGR_LAST_NM       
FROM        
  EMP_DETAILS_VIEW EDV,       
  EMPLOYEES EMP,       
  LOCATIONS LOC       
WHERE        
  EDV.EMPLOYEE_ID = EMP.EMPLOYEE_ID       
  AND EDV.LOCATION_ID = LOC.LOCATION_ID      
  AND EMP.COMMISSION_PCT IS NOT NULL   
  --AND EMP.EMPLOYEE_ID IN(149)]]></SQLQuery>
