<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="NewEmpMgmtTF">
    <default-activity>NewEMView</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
   
    <managed-bean id="__1">
      <managed-bean-name>Show_Hide</managed-bean-name>
      <managed-bean-class>com.silbury.view.beans.ShowHide</managed-bean-class>
      <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean id="__2">
      <managed-bean-name>SearchBean</managed-bean-name>
      <managed-bean-class>com.silbury.view.beans.onSearch</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean id="__3">
      <managed-bean-name>CreateEmpHelper</managed-bean-name>
      <managed-bean-class>com.silbury.view.beans.CreateEmployeeHelper</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <view id="NewEMView">
      <page>/Views/NewEMView.jsff</page>
    </view>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
